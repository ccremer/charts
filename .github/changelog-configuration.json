{
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": [
        "enhancement",
        "feature"
      ]
    },
    {
      "title": "## 🛠️ Minor Changes",
      "labels": [
        "change"
      ]
    },
    {
      "title": "## 🔎 Breaking Changes",
      "labels": [
        "breaking"
      ]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": [
        "bug",
        "fix"
      ]
    },
    {
      "title": "## 📄 Documentation",
      "labels": [
        "documentation"
      ]
    },
    {
      "title": "## 🔗 Dependency Updates",
      "labels": [
        "dependency"
      ]
    }
  ],
  "template": "${{CATEGORIZED_COUNT}} changes since ${{FROM_TAG}}\n\n${{CHANGELOG}}",
  "label_extractor": [
    {
      "pattern": "<from-workflow>",
      "method": "replace",
      "target": "ignore",
      "on_property": "title"
    }
  ],
  "tag_resolver": {
    "filter": {
      "pattern": "<from-workflow>"
    },
    "transformer": {
      "pattern": "<from-workflow>",
      "target": "$1"
    }
  }
}
